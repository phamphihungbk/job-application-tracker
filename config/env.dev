## APP INFO
APP_ENV=development
APP_PORT=3000
RATE_LIMIT_REQUEST: 10

## DATABASE_INFO
TYPEORM_CONNECTION=postgres
TYPEORM_HOST=postgres-app-tracker
TYPEORM_PORT=5432
TYPEORM_USERNAME=tracker_user
TYPEORM_PASSWORD=tracker_password
TYPEORM_DATABASE=tracker_db
TYPEORM_SYNCHRONIZE=false
TYPEORM_LOGGING=TRUE
TYPEORM_ENTITIES=src/models/*{.ts,.js}
TYPEORM_MIGRATIONS=database/migrations/*{.ts,.js}
TYPEORM_DRIVER_EXTRA='{"bigNumberStrings": false}'
TYPEORM_SEEDING_FACTORIES=database/factories/**/*{.ts,.js}
TYPEORM_SEEDING_SEEDS=database/seeds/*{.ts,.js}
TYPEORM_ENTITIES_DIR=src/models

## DOCKER ENV
POSTGRES_USER=root
POSTGRES_PASSWORD=secret
