{
  "name": "job-application-tracker-backend",
  "version": "1.0.0",
  "description": "Backend for job application tracker",
  "author": "HUNG PHAM",
  "scripts": {
    "dev": "nodemon --exec './node_modules/.bin/ts-node' main.ts",
    "start": "npm run build && node ./dist/main.js",
    "build": "tsc",
    "typeorm": "ts-node -r ./fix-module-alias.js ./node_modules/.bin/typeorm",
    "make:migration": "npm run typeorm -- migration:create -n",
    "migrate": "npm run typeorm -- migration:run",
    "migrate:revert": "npm run typeorm -- migration:revert",
    "db:seed": "ts-node -r ./fix-module-alias.js ./node_modules/typeorm-seeding/dist/cli.js seed",
    "code:format": "prettier --write \"src/**/*.{ts,css,js,html}\"",
    "code:check": "prettier --check \"src/**/*.{ts,css,js,html}\""
  },
  "dependencies": {
    "@types/socket.io": "^2.1.13",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "class-validator-jsonschema": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "helmet": "^4.6.0",
    "module-alias": "^2.2.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "socket-controllers": "0.0.5",
    "socket.io": "^3.0.0",
    "supertest": "^6.1.3",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.34",
    "typeorm-seeding": "^1.6.1",
    "typeorm-simple-query-parser": "^1.0.13",
    "typeorm-typedi-extensions": "^0.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^14.17.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "jest": "^27.0.6",
    "nodemon": "^2.0.11",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "~11.13.0"
  }
}
